  "browserslist": [
    "last 2 versions"
  ],
  "lint-staged": {
    "{packages,stories,.storybook}/**/*.(sa|sc|c)ss": "stylelint --fix",
    "*.js": "eslint --fix"
  },
  "dependencies": {
