{% extends "layout.njk" %}
{% from "./component.njk" import list_group %}
{% from "./../format.njk" import format %}

{% block content %}

    <h2>default</h2>
    {% set example %}
    {{  list_group({ items: data.properties.items }) }}
    {% endset %}
    {{ format(example) }}

    <br/><br/>
    <h2>with actions</h2>
    {% set example %}
    {{ list_group(data.properties) }}
    {% endset %}
    {{ format(example) }}

{% endblock %}