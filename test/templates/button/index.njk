}}
                            {{-  anchor_button(
  { label: 'Disabled',
    icon: 'cart' },
    modifier={ block: [type,size,e] },
    state={ disabled: true })
}}
                            {% endset %}
                            {{ btncode(example) }}

                            <br>
                        {% endfor %}

                    </td>
                {% endfor %}
            {% endfor %}
        </tr>
    </table>

