          "properties": {
            "class": {
              "const": "fd-object-list__content",
              "description": "Element class name"
            }
          }
        },
        "fd-object-list__object-number": {
          "type": "object",
          "description": "object-number element",
          "properties": {
            "class": {
              "const": "fd-object-list__object-number",
              "description": "Element class name"
            }
          }
        },
        "fd-object-list__object-identifier": {
          "type": "object",
          "description": "object-identifier element",
          "properties": {
            "class": {
              "const": "fd-object-list__object-identifier",
              "description": "Element class name"
            }
          }
        },
        "fd-object-list__object-attribute": {
          "type": "object",
          "description": "object-attribute element",
          "properties": {
            "class": {
              "const": "fd-object-list__object-attribute",
              "description": "Element class name"
            }
          }
        }
      },
      "additionalProperties": false
